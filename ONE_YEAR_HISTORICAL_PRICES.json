  "one_year_calculations": {
    "description": "Cost of each item as fraction of 1Y (median household income)",
    "methodology": "Price divided by median household income for that year",
    "sample_calculations": {
      "median_home": {
        "1950": {
          "price": 7354,
          "income": 3216,
          "years_of_work": 2.29,
          "months_of_work": 27.4
        },
        "2025": {
          "price": 409200,
          "income": 83730,
          "years_of_work": 4.89,
          "months_of_work": 58.6
        },
        "change": "Home costs MORE than DOUBLED in years-of-labor (2.29Y → 4.89Y)"
      },
      "college_tuition_4yr": {
        "1970": {
          "price_4yr": 1576,
          "income": 9867,
          "years_of_work": 0.16,
          "months_of_work": 1.9
        },
        "2025": {
          "price_4yr": 47800,
          "income": 83730,
          "years_of_work": 0.57,
          "months_of_work": 6.8
        },
        "change": "College (4 years) went from 2 months to 7 months of work"
      },
      "new_car": {
        "1970": {
          "price": 3450,
          "income": 9867,
          "years_of_work": 0.35,
          "months_of_work": 4.2
        },
        "2025": {
          "price": 48644,
          "income": 83730,
          "years_of_work": 0.58,
          "months_of_work": 7.0
        },
        "change": "New car went from 4 months to 7 months of work"
      },
      "dozen_eggs": {
        "1970": {
          "price": 0.60,
          "income": 9867,
          "annual_consumption_cost": 31.20,
          "fraction_of_1Y": 0.003
        },
        "2025": {
          "price": 2.86,
          "income": 83730,
          "annual_consumption_cost": 148.72,
          "fraction_of_1Y": 0.002
        },
        "note": "Eggs have actually become MORE affordable relative to income"
      }
    }
  },

